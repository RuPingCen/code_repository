<launch>

  
  <node pkg="sts3215_driver" type="sts3215_driver" name="sts3215_driver" output="screen">
 	  <param name="dev" value="/dev/ttyUSB0" type="str" />
	  <param name="buad" value="1000000" type="int" />
	  <param name="time_out" value="1000" type="int" />
	  <param name="hz" value="10" type="int" />
	  <param name="show_message" value="1" type="int" />
  </node>
 

</launch>
